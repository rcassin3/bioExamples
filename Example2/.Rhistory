library(tidyverse)
library(dplyr)
library(ggplot2)
#read in air temperature data
global_temp.df  <- read.csv("global_temp_cleaned.csv")
# co2 dat set
loa_co2.df <- read.csv("loa_co2_cleaned.csv")
# Number 4
# you should clean up figure
windows()
ggplot(data = global_temp.df, aes(x=year,
y = annual_temp_c,))+
geom_point()+
theme_light()+
geom_smooth(method = "loess", color = "red") +
labs(title = "Annual Temperature Change from Baseline 1880-2021" , y = "Annual Temperature Change from Baseline (°C)", x = "Year") +
theme(text = element_text(size = 19), plot.title = element_text(hjust = 0.5))
options(scipen = 999)
regression <- lm(global_temp.df$annual_temp_c ~
global_temp.df$year)
summary(regression)
